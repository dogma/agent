<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="runtimeConfigDAO" class="au.edu.une.monitor.web.RuntimeConfigDAOPropertiesFileImpl">
        <property name="runtimePropertiesFileDirectory" value="${catalina.base}/cabinet" />
        <property name="runtimePropertiesFileName" value="agent.runtime.properties" />
        <property name="defaultState" ref="defaultState" />
    </bean>

</beans>